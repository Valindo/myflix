{
  "manifest_version": 2,
  "name": "Myflix",
  "version": "2.1",
  "minimum_chrome_version": "23",
  "icons": {
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    {
      "fileSystem": ["write", "retainEntries", "directory"]
    },
    "unlimitedStorage",
    "http://ia.media-imdb.com/"
  ]
}